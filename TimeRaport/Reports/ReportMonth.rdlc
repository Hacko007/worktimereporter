      <Query>
        <DataSourceName>TimeLogDBConnectionString</DataSourceName>
        <CommandText>SELECT     TimeLog.Day, TimeLog.CategoryId, TimeLog.InTime, TimeLog.OutTime, TimeLog.Info, 
                      Category.Company + ' - ' + Category.Project + ' - ' + Category.Code AS CategoryText, TimeLog.TimeLogId
FROM         TimeLog INNER JOIN
                      Category ON TimeLog.CategoryId = Category.CategoryId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
